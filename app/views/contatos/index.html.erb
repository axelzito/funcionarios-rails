<p id="notice"><%= notice %></p>

<h1>Contatos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Matricula</th>
      <th>Nascimento</th>
      <th>Municipio</th>
      <th>Estado</th>
      <th>Civil</th>
      <th>Sexo</th>
      <th>Job</th>
      <th>Workspace</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contatos.each do |contato| %>
      <tr>
        <td><%= contato.nome %></td>
        <td><%= contato.matricula %></td>
        <td><%= contato.nascimento %></td>
        <td><%= contato.municipio %></td>
        <td><%= contato.estado %></td>
        <td><%= contato.civil %></td>
        <td><%= contato.sexo %></td>
        <td><%= contato.job %></td>
        <td><%= contato.workspace %></td>
        <td><%= link_to 'Show', contato %></td>
        <td><%= link_to 'Edit', edit_contato_path(contato) %></td>
        <td><%= link_to 'Destroy', contato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contato', new_contato_path %>
